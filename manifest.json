{
  "name": "APRSTime",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "An autosplitter for Agent P: Rebel Spy",
  "homepage_url": "https://github.com/eiim/APRSTime",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
	"96": "icons/icon96.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "options_page": "src/options_custom/index.html",
  "browser_action": {
    "default_icon": {
	  "16": "icons/icon16.png",
	  "48": "icons/icon48.png",
	  "96": "icons/icon96.png"
	},
    "default_title": "page action demo",
    "default_popup": "src/page_action/page_action.html"
  },
  "permissions": [
    "contentSettings",
    "https://lol.disney.com/games/agent-p-rebel-spy*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://lol.disney.com/games/agent-p-rebel-spy*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}